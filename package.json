{
  "name": "ashudevwebsite",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --",
    "start:dist": "http-server dist --port ${PORT:-8080}",
    "build": "webpack --config webpack.prod.js",
    "preview": "npm run build && npm run start:dist",
    "test": "standard",
    "docker:init": "DOCKER_BUILDKIT=1 docker build --rm --target dev -t ashudev-website-dev:latest . && docker run -it --rm -v $PWD:/home/node/app ashudev-website-dev npm i",
    "docker:start": "DOCKER_BUILDKIT=1 docker run -it --rm -v $PWD:/home/node/app -p 8080:8080 ashudev-website-dev npm start",
    "docker:dist": "DOCKER_BUILDKIT=1 docker run -it --rm -v $PWD:/home/node/app -p 8080:8080 ashudev-website-dev npm run start:dist",
    "docker:build": "DOCKER_BUILDKIT=1 docker run -it --rm -v $PWD:/home/node/app -p 8080:8080 ashudev-website-dev npm run build",
    "docker:preview": "DOCKER_BUILDKIT=1 docker run -it --rm -v $PWD:/home/node/app -p 8080:8080 ashudev-website-dev npm run preview",
    "docker:test": "DOCKER_BUILDKIT=1 docker run -it --rm -v $PWD:/home/node/app -p 8080:8080 ashudev-website-dev npm test"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
